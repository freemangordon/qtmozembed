<?xml version="1.0" encoding="UTF-8"?>
<testdefinition version="1.0">
   <suite name="qtmozembed-tests" domain="ui">
       <description>QtMozEmbed Application automatic tests</description>
       <set name="unit-tests" feature="QtMozEmbed">
